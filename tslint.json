{
  "rules": {
    "class-name": true,
    "no-var-keyword": true,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ],
    "no-internal-module": true,
    "prefer-const": true,
    "jsdoc-format": true,
    "no-console": [false, "log"],
    "no-any": false,
    "semicolon": [false, "never"],
    "quotemark": [true, "single"]
  }
}